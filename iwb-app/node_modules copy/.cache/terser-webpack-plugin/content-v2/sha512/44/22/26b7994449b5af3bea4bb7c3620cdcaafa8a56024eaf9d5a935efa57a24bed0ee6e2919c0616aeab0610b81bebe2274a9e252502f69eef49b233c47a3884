in.css'; // Import Font Awesome CSS\nimport './App.css';\n\nfunction App() {\n  const [userRole, setUserRole] = useState(null);\n\n  useEffect(() => {\n    const user = JSON.parse(localStorage.getItem('user'));\n    if (user) {\n      setUserRole(user.role);\n    }\n  }, []);\n\n  return (\n    <Router>\n      <div className=\"App\">\n        <Navbar setUserRole={setUserRole} /> {/* Add Navbar */}\n        <Routes>\n          <Route path=\"/\" element={<Login setUserRole={setUserRole} />} />\n          <Route path=\"/register\" element={<Register />} />\n          <Route path=\"/login\" element={<Login setUserRole={setUserRole} />} />\n\n          <Route path=\"/dashboard\" element={\n            userRole === 'investor' ? <InvestorDashboard /> :\n            userRole === 'finance' ? <FinanceDashboard /> :\n            userRole === 'developer' ? <DeveloperDashboard /> :\n            userRole === 'client' ? <ClientDashboard /> :\n            userRole === 'primary_partner' ? <PrimaryPartnerDashboard /> :\n            userRole === 'sales' ? <SalesDashboard /> : null\n          } />\n\n          <Route path=\"/about\" element={<About />} />\n          <Route path=\"/company\" element={<Company />} />\n          <Route path=\"/overview\" element={<Overview />} />\n          <Route path=\"/contact-us\" element={<ContactUs />} />\n        </Routes>\n      </div>\n    </Router>\n  );\n}\n\nexport default App;","import React from 'react';\nimport ReactDOM from 'react-dom'; // Change to react-dom\nimport App from './App'; // Ensure App is imported correctly\nimport './index.css';\n\nconst root = document.getElementById('root');\nReactDOM.render(\n    <React.StrictMode>\n        <App />\n    </React.StrictMode>,\n    root\n);"]}}